#include <iostream>
#include <string>
using namespace std;

int main(void)	{
	std::string temp;
	cin >> temp;
	
	std::string operand;
	
	std::string easy = "cCxX";

	std::size_t first = temp.find("'");
	std::size_t last = temp.rfind("'", temp.size());
	
	if (temp.compare(first-1, 1, "c") == 0)
		cout << "YES" << endl;
		
	if(first==std::string::npos || last==std::string::npos)	{
		cout << "Error, no operand found" << endl;
	 }
	 else if (first == last)	{
	 	cout << "Error, missing second quote" << endl;
	 }
	 else {
	 	switch (temp[first-1]) {
			case 'c':
			case 'C':
			case 'x':
			case 'X':	{
				operand = temp.substr(first-1, last-first+1);
				break;	
			}
			default: 	{
				operand = temp.substr(first, last-first+1);
				break;
			}
		}
	 }
	return 0;
}


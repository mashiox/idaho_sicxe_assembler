#include <iostream>
#include <string>
#include <map>
#include "opcodetab.h"
using namespace std;
int main(void) 	{
	
	string op = "+add";
	char c, t;
	int i = 0;
	
	if (op[0] == '+')	{
		op = op.erase(0,1);
	}
	while(op[i])	{
		c = op[i];
		t = toupper(c);
		op[i] = t;
		i++;
	}
	map <string, struct opcodes*> opcodeTab;
	
	if(opcodeTab.find(op) == opcodeTab.end())	{
		cout << "Exception" << endl;
	}
	else {
		struct opcodes* temp = opcodeTab[op];
		cout << temp->opcode << endl;
	}
	return 0;	
}
